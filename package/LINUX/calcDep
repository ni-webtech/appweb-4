cd /tmp/appwebPkg

objdump -p binDist/etc/appweb/lib/* binDist/usr/local/bin/* | 
	grep NEEDED | 
	awk '{ print $2 }' | 
	egrep -v 'libesp|libegi|libsslModule|libappweb.so|matrixssl' | 
	sort | 
	uniq | 
while read x
do
	#echo dpkg -S $x
	dpkg -S $x
done | awk '{ print $1 }' | sed 's/://' | sort | uniq
